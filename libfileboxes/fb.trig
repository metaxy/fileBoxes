@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix nrl:  <http://www.semanticdesktop.org/ontologies/2007/08/15/nrl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix nie:  <http://www.semanticdesktop.org/ontologies/2007/01/19/nie#> .
@prefix fb:   <http://paul.walger.eu/ontologies/fb#> .

fb: {

 

    fb:FileBox
        a rdfs:Class ;
        rdfs:label "filebox";
        rdfs:comment "Represents a fileBox" ;
        rdfs:subClassOf rdfs:Resource .

	fb:isPartOfFileBox
        a rdf:Property ;
        rdfs:label "isPartOfFileBox";
        rdfs:comment "Is in file box." .

    fb:boxID
        a rdf:Property ;
        rdfs:label "box ID" ;
        rdfs:comment "The ID of the box" .

    fb:hasNewFiles
        a rdf:Property ;
        rdfs:label "has new files" ;
        rdfs:range xsd:boolean ;
        rdfs:comment "If it has new files" .

    fb:isFileBoxIn
        a rdf:Property ;
        rdfs:label "isFileBoxIn" ;
        rdfs:comment "Is a fileboxes in Ex. fileboxes:/" .
        
}